{
  "serviceCore": {
    "daoFile": "config/dao.config",
    "servicesFile": "config/Components.xml"
  },
  "Caches": {
    "LoginResult": {
      "CacheID": "LoginResult",
      "Type": "LRURedis",
      "Host": "127.0.0.1",
      "Port": 6379,
      "lifeTime": {
        "hours": 12,
        "min": 0,
        "seconds": 0
      },
      "Size": 256
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
