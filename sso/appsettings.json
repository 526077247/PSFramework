{
  "dao": {
    "file": "config/dao.config"
  },
  "servicesfile": "Components.xml",
  "Caches": {
    "LoginResult": {
      "CacheID": "LoginResult",
      "Type": "Redis",
      "Host": "127.0.0.1",
      "Port": 6379,
      "lifeTime": {
        "hours": 12,
        "min": 0,
        "seconds": 0
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
